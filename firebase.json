{
  "firebase": "javabrains",
  "public": "dist",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "/user",
      "destination": "/user_management.html"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
}